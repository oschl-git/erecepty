<% if (successMessage) { %>
<div class="success">
  <h4><%= successMessage %></h4>
</div>
<% } %>

<% if (formErrors.length) { %>
<div class="error">
  <h4>Chyba, záznam nemohl být přidán z následujících důvodů:</h4>
  <ul>
    <% for (const error of formErrors) { %>
    <li><%= error %></li>
    <% } %>
  </ul>
</div>
<% } %>